version: '3'
services:
  python-app:
    build:
      context: ./ProjetMeteo

  # Remplacez par le chemin r√©el
    depends_on:
      - cassandra
    environment:
      - CASSANDRA_HOST=cassandra
    networks:
      - mon-reseau

  cassandra:
    image: cassandra:latest
    ports:
      - "9042:9042"
    networks:
      - mon-reseau

networks:
  mon-reseau:
